{
  "expo": {
    "name": "app-expo",
    "slug": "app-expo",
    "scheme": "acme",
    "icon": "./src/assets/images/icon.png",
    "web": {
      "output": "server",
      "bundler": "metro"
    },

    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/FiraSans-MediumItalic.ttf",
            "./src/assets/fonts/Verdana.ttf",
            "./src/assets/fonts/Verdana-Bold.ttf",
            "./src/assets/fonts/Andale.ttf"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./src/assets/images/splash-icon.png",
          "dark": {
            "image": "./src/assets/images/splash-icon-dark.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    },

    "android": {
      "package": "com.kentoverse.appexpo"
    },

    "ios": {
      "bundleIdentifier": "com.kentoverse.appexpo"
    },
    "extra": {
      "expoPublicNounKey": "process.env.EXPO_PUBLIC_NOUN_KEY",
      "expoPublicNounSecret": "process.env.EXPO_PUBLIC_NOUN_SECRET",
      "router": {
        "origin": "https://n"
      },
      "eas": {
        "projectId": "0efb980d-7fb4-4560-ad1f-ce60df778400"
      },
      "owner": "bot-digital-works",
      "API_BASE_URL": "process.env.MCP_ENDPOINT",
      "APPLICATIONINSIGHTS_CONNECTION_STRING": "..."
    }
  }
}

